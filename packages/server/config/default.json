{
  "authentication": {
    "authStrategies": [ "jwt", "local" ],
    "entity": "user",

    "jwtOptions": {
      "header": { "typ": "access" },
      "audience": "https://yourdomain.com",
      "issuer": "feathers",
      "algorithm": "HS256",
      "expiresIn": "1d"
    },

    "local": {
      "usernameField": "username",
      "passwordField": "password"
    },

    "secret": "",

    "service": "users"
  },
  "host": "localhost",
  "memory": {
    "id": "_id"
  },
  "nedb": "../../../data/server",
  "port": 3030,
  "protocol": "https",
  "public": "../../../dist/server",
  "paginate": {
    "default": 10,
    "max": 50
  }
}
